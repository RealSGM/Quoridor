; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Quoridor"
run/main_scene="res://scenes/main/main.tscn"
config/features=PackedStringArray("4.3", "C#", "Mobile")
config/icon="res://icon.png"

[autoload]

Resources="*res://autoload/resources.tscn"
SignalManager="*res://autoload/signal_manager.gd"
FenceManager="*res://autoload/fence_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="ignore"

[dotnet]

project/assembly_name="Quoridor"
